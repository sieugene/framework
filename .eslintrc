{
    "parser": "babel-eslint",
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "rules": {
        "quotes": 0,
        "semi":"off",
        "linebreak-style":"off",
        "object-curly-spacing":"off",
        "require-jsdoc":"off",
        "no-debugger": "off",
        "comma-spacing": "off",
        "spaced-comment": "off",
        "padded-blocks": "off",
        "no-trailing-spaces": "off",
        "arrow-parens": "off",
        "operator-linebreak": "off",
        "indent":"off",
        "space-before-function-paren": "off"
    },
    "extends": ["eslint:recommended", "google"]
}